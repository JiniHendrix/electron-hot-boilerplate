{
  "name": "electron-quick-start",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "jsx": "jsx -x jsx src/ out/",
    "start": "cross-env NODE_ENV=development electron main.js",
    "test": "mocha --use_strict",
    "build": "browserify -t reactify --node --no-bundle-external src/index.js -o out/bundle.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/atom/electron-quick-start.git"
  },
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/atom/electron-quick-start/issues"
  },
  "homepage": "https://github.com/atom/electron-quick-start#readme",
  "devDependencies": {
    "browserify": "^13.0.0",
    "cross-env": "^1.0.7",
    "electron-prebuilt": "^0.36.0",
    "enzyme": "^2.0.0",
    "escodegen": "^1.8.0",
    "esprima": "^2.7.2",
    "estraverse": "^4.1.1",
    "expect": "^1.14.0",
    "gulp": "^3.9.1",
    "gulp-processhtml": "^1.1.0",
    "gulp-util": "^3.0.7",
    "jstransform": "^11.0.3",
    "mocha": "^2.4.5",
    "react-addons-test-utils": "^0.14.7",
    "react-deep-force-update": "^2.0.1",
    "react-proxy": "^2.0.3",
    "reactify": "^1.1.1",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watch-glob": "^0.1.3"
  },
  "dependencies": {
    "react": "^0.14.7",
    "react-dom": "^0.14.7"
  }
}
